{
  "include": ["src"],
  "exclude": [],
  "compilerOptions": {
    "declaration": true,
    "sourceMap": true,
    "outDir": "lib",
    "target": "es2018",
    "module": "commonjs",
    "lib": [
      "es2020.string"
    ],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
